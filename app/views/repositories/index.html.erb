<div class = "container">
  <h1>Repos</h1>
  <%= @user_data["login"] %>

  <% @repo_data.each dp |repo| %>
  <%= link_to repo["name"], "https://www.github.com/#{@user_data["login"]}/#{r}["name"]}" %>  
  <% end %>
  
  <!-- List repositories and display username here! -->

  <%= form_tag("/repositories/create", method: "post") do %>
    <%= label_tag(:name, "Create a New Repo:") %>
    <%= text_field_tag(:name, nil, id: "new-repo") %>
    <%= submit_tag("Create") %>
  <% end %>
</div>
